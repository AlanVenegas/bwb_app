<% provide(:title, "Acceder") %>
<h1>Acceder</h1>
<%= form_for(:session, url: sessions_path) do |f| %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.submit "Acceder", class: "btn btn-primary" %>
<% end %>

<p>¿Aún no eres usuario? <%= link_to "Ir al Registro", signup_path %></p>
